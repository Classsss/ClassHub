@page "/classroom/{roomid:int}/practice"
@layout MainLayout
@using ClassHub.Client.Models
@inject HttpClient Http

<SubmissionList Title="실습" Assignment="@practices" />


@code {
    [Parameter]
    public int Id { get; set; }
    private Assignment? practices;

    protected async override void OnInitialized()
    {
        // RoomId와 매칭되는 강의실의 정보를 가져옵니다.
        practices = await Http.GetFromJsonAsync<Assignment>($"api/classroom/{Id}");
        StateHasChanged();
    }
    
}
